# Server Configuration
PORT=3000
NODE_ENV=development

# Azure DevOps Configuration
AZURE_DEVOPS_ORG_URL=https://dev.azure.com/your-organization
AZURE_DEVOPS_TOKEN=your-personal-access-token
AZURE_DEVOPS_PROJECT=your-project-name
AZURE_DEVOPS_REPOSITORY_ID=your-repository-id

# Webhook Configuration
WEBHOOK_SECRET=your-webhook-secret-key

# Google Gemini AI Configuration
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-pro

# Git Configuration
GIT_USER_NAME=Redimento Code Generator
GIT_USER_EMAIL=redimento@yourcompany.com
GIT_DEFAULT_BRANCH=main

# Repository Configuration
REPOS_BASE_PATH=./repos
DEFAULT_REVIEWERS=reviewer1@company.com,reviewer2@company.com

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# Retry Configuration
MAX_RETRY_ATTEMPTS=3
RETRY_BASE_DELAY=1000
RETRY_MAX_DELAY=10000

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=5000

# Security Configuration
API_KEY=your-api-key-for-internal-endpoints